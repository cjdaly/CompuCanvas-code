<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="C3Product"
         point="org.eclipse.core.runtime.products">
      <product
            application="net.locosoft.CompuCanvas.controller.packaging.C3Application"
            name="CompuCanvas Controller">
         <property
               name="appName"
               value="CompuCanvas Controller">
         </property>
      </product>
   </extension>
   <extension
         id="C3Application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="net.locosoft.CompuCanvas.controller.packaging.internal.C3Application">
         </run>
      </application>
   </extension>

</plugin>
